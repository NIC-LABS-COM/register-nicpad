<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.f" controllerName="sap.ui.demo.todo.controller.Home" displayBlock="false">
	<Page title="{i18n>homeTitle}" showNavButton="false" navButtonPress=".onNavBack">
		<content>
			<VBox fitContainer="true">
				<items>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>peopleRegistrationTile}" press="onPerson" frameType="Auto">
						<TileContent>
							<ImageContent src="sap-icon://employee" />
						</TileContent>
					</GenericTile>
				</items>
			</VBox>
		</content>
	</Page>
</mvc:View>